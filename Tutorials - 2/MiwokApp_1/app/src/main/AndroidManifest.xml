<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pritish.miwokapp_1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Numbers"
            android:label="@string/numbers"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".Colors"
            android:label="@string/colors"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".FamilyNumbers"
            android:label="@string/family"
            android:parentActivityName=".FamilyNumbers" />

        <activity android:name=".Phrases"
            android:label="@string/phrases"
            android:parentActivityName=".Phrases" />

    </application>

</manifest>